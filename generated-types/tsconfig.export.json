{
	"compilerOptions": {
		"outDir": "./extension",
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"module": "ESNext",
		"moduleResolution": "bundler",
		"target": "ES2020",
		"lib": ["ES2020", "DOM"],
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": false
	},
	"include": ["./export.ts"]
}
